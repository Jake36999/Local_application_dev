{
  "staging": {
    "enabled": true,
    "incoming_dir": "staging/incoming/",
    "processed_dir": "staging/processed/",
    "failed_dir": "staging/failed/",
    "scan_interval_seconds": 5,
    "retention_days": 30,
    "auto_cleanup": true
  },
  "workflows": {
    "auto_run": [
      "ingest",
      "cut_analysis",
      "governance"
    ],
    "max_concurrent": 3,
    "timeout_seconds": 300
  },
  "sentinel": {
    "binary_paths": {
      "tesseract": "C:\\Program Files\\Tesseract-OCR\\tesseract.exe",
      "poppler": "C:\\Program Files\\poppler-24.02.0\\Library\\bin\\pdftoppm.exe"
    },
    "enforce_typing": true,
    "auto_repair_imports": false,
    "min_text_density": 10
  },
  "ui": {
    "enabled": true,
    "port": 8501
  },
  "logging": {
    "level": "INFO",
    "log_file": "logs/orchestrator.log"
  }
}