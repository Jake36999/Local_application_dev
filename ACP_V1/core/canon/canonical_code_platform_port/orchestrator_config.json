{
  "staging": {
    "enabled": true,
    "incoming_dir": "staging/incoming/",
    "processed_dir": "staging/processed/",
    "failed_dir": "staging/failed/",
    "scan_interval_seconds": 5,
    "retention_days": 30,
    "auto_cleanup": true
  },
  "workflows": {
    "auto_run": [
      "ingest",
      "cut_analysis",
      "governance"
    ],
    "max_concurrent": 3,
    "timeout_seconds": 300
  },
  "ui": {
    "enabled": true,
    "port": 8501
  },
  "logging": {
    "level": "INFO",
    "log_file": "orchestrator.log"
  }
}